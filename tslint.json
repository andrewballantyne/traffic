{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "typedef-whitespace": [
      true,
      {
        "variable-declaration": "nospace"
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-typecast"
    ]
  }
}